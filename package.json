{
	"name": "prognosis",
	"displayName": "Prognosis",
	"description": "A VSCode native game engine",
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:prognosis.start"
	],
	"main": "./src/EditorManager.js",
	"contributes": {
		"commands": [
			{
				"command": "prognosis.start",
				"title": "Prognosis: Start"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "prognosis-explorer",
					"title": "Prognosis Explorer",
					"icon": "assets/images/bumblebee.jpg"
				}
			]
		},
		"views": {
			"prognosis-explorer": [
				{
					"id": "prognosis-scenes",
					"name": "Scenes"
				},
				{
					"id": "prognosis-assets",
					"name": "Assets"
				}
			]
		}
	},
	"dependencies": {
		"vscode": "^0.9.9"
	},
	"devDependencies": {
		"prettier": "2.4.1"
	}
}
